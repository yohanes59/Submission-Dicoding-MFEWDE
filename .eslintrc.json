{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "indent":"off",
        "no-underscore-dangle":"off",
        "no-tabs":"off",
        "quotes":"off",
        "dot-notation":"off",
        "no-console":"off",
        "no-restricted-globals":"off",
        "no-prototype-builtins":"off",
        "consistent-return":"off",
        "no-unused-expressions":"off",
        "import/no-extraneous-dependencies": "off",
        "no-undef": "off",
        "import/prefer-default-export": "off"
    }
}
